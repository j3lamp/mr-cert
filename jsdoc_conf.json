{
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-export-default-interop/dist/index"
    ],

    "source": {
        "include": [
            "client",
            "common",
            "server"
        ]
    },

    "tags": {
        "allowUnknownTags": false
    },

    "opts": {
        "recurse":     true,
        "readme":      "README.md",
        "access":      "all",
        "destination": "dist/docs",
        "template":    "node_modules/docdash"
    }
}
